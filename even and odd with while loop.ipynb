   "source": [
    "i=0\n",
    "while i<=9:\n",
    "    if (i%2==0):\n",
    "        print(\"{} as even\".format(i))\n",
    "        i=i+1\n",
    "    else:\n",
    "        print(\"{} as odd\".format(i))\n",
    "        i=i+1"
